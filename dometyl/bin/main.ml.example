open! Base
open! Scad_ml
open! Generator
open! Boards


let splaytyl = Splaytyl.build ()

let () =
  Stdio.print_endline "Building (and maybe exporting) scads...";
  Write.thing "splaytyl" (Case.to_scad ~show_cutouts:false ~show_caps:true splaytyl);
  Write.thing "bottom_plate" (Bottom.make splaytyl);
  Write.thing "tent" (Tent.make splaytyl);
  Stdio.print_endline "Done!"
